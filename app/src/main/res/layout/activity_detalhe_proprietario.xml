<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.edu.iff.pooa20171.exerciciorealm.Activity.DetalheProprietarioActivity">

    <EditText
        android:id="@+id/edNome"
        android:layout_width="374dp"
        android:layout_height="49dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="0dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nome"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        tools:layout_editor_absoluteX="3dp" />

    <EditText
        android:id="@+id/edEndereco"
        android:layout_width="522dp"
        android:layout_height="60dp"
        android:layout_marginTop="0dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Endereco"
        app:layout_constraintTop_toBottomOf="@+id/edNome"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.051" />

    <EditText
        android:id="@+id/etBairro"
        android:layout_width="379dp"
        android:layout_height="53dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Bairro"
        tools:layout_editor_absoluteY="111dp"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/etCidade"
        android:layout_width="384dp"
        android:layout_height="57dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Cidade"
        tools:layout_editor_absoluteY="164dp"
        tools:layout_editor_absoluteX="-4dp" />

    <EditText
        android:id="@+id/etCNH"
        android:layout_width="388dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="CNH"
        tools:layout_editor_absoluteY="221dp"
        tools:layout_editor_absoluteX="0dp" />


    <EditText
        android:id="@+id/etTelefone"
        android:layout_width="384dp"
        android:layout_height="47dp"
        android:ems="10"
        android:inputType="phone"
        android:text="Telefone"
        tools:layout_editor_absoluteY="264dp"
        tools:layout_editor_absoluteX="-4dp" />

    <EditText
        android:id="@+id/edEmail"
        android:layout_width="388dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="Email"
        tools:layout_editor_absoluteY="311dp"
        tools:layout_editor_absoluteX="-2dp" />

    <EditText
        android:id="@+id/edLatitude"
        android:layout_width="387dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Latitude"
        tools:layout_editor_absoluteY="358dp"
        tools:layout_editor_absoluteX="1dp" />

    <EditText
        android:id="@+id/etLongitude"
        android:layout_width="381dp"
        android:layout_height="47dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Longitude"
        tools:layout_editor_absoluteY="404dp"
        tools:layout_editor_absoluteX="1dp" />

    <Button
        android:id="@+id/btSalvar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Salvar"
        tools:layout_editor_absoluteY="462dp"
        tools:layout_editor_absoluteX="25dp" />

    <Button
        android:id="@+id/btAlterar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Alterar"
        tools:layout_editor_absoluteY="462dp"
        tools:layout_editor_absoluteX="133dp" />

    <Button
        android:id="@+id/btDeletar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="57dp"
        android:text="Deletar"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="462dp" />

</android.support.constraint.ConstraintLayout>
