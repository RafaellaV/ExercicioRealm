<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.edu.iff.pooa20171.exerciciorealm.Activity.ProprietarioActivity">

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_editor_absoluteX="213dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/tvProprietarios"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lista de Proprietarios:"
        app:layout_constraintTop_toBottomOf="@+id/fab"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/rvProprietarios"
        android:layout_width="280dp"
        android:layout_height="233dp"
        tools:layout_editor_absoluteX="16dp"
        app:layout_constraintTop_toBottomOf="@+id/tvProprietarios" />
</android.support.constraint.ConstraintLayout>
